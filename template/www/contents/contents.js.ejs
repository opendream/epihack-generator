//<script>// this tag is be here, just to make an editor renderer syntax correctly.
angular.module('epihack')

.config(function ($stateProvider, $urlRouterProvider) {

        $stateProvider

        <% pages.forEach(function(page){ %>
            .state('app.<%= page.id %>', {
                url: '/contents/<%= page.id %>',
                views: {
                    'menuContent': {
                        templateUrl: "contents/templates/<%= page.id %>.html"
                    }
                }
            })
        <% }); %>

});
//</script>
